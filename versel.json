{
    "builds":[{
        "src": "organizer_fina_api.wsgi.py",
        "use": "@vercel/python",
        "config":{
            "maxLambdaSize": "50mb",
            "runtime": "python3.10.6"
        }
    }],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "organizer_fina_api.wsgi.py"
        }
    ]
}

{
    "version": 2,
    "builds": [
      {
        "src": "organizer_fina_api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "organizer_fina_api/wsgi.py"
      }
    ]